<fx:root fx:id="addItemBox" prefHeight="560.0" prefWidth="894.0" style="-fx-background-color: #007;" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AddItemController">
